<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.juntai.look.homePage.mydevice.allGroup.camerasOfGroup.CamerasListActivity">

    <include
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toTopOf="@+id/transfer_dev_tv"
        layout="@layout/recycleview_layout"></include>

    <TextView
        android:id="@+id/transfer_dev_tv"
        style="@style/bt_style"
        android:layout_gravity="center"
        android:layout_marginBottom="20dp"
        android:text="转入设备"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"></TextView>
</android.support.constraint.ConstraintLayout>
